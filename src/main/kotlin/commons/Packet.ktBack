package commons

class Packet(val command: SystemCommand = SystemCommand.Ping) {
    var args = mutableMapOf<PacketKeys, Any>()

    companion object {
        enum class PacketKeys {
            Sender,
            Card,
            Message,
            commons.GameFrame
        }


        enum class SystemCommand {
            Init,
            Quit,
            Ping,
        }
    }
}
